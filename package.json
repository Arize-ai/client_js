{
  "name": "@arizeai/ax-client",
  "version": "1.0.2",
  "description": "A Typescript client for the Arize AX API",
  "main": "dist/src/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/src/index.js"
    }
  },
  "files": [
    "dist",
    "package.json"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "pnpm run clean && pnpm run generate",
    "generate": "openapi-typescript --empty-objects-unknown=true --default-non-nullable=false ../../../../openapi/openapi.yaml -o ./src/__generated__/api/v2.ts",
    "build": "tsc -p tsconfig.json && tsc -p tsconfig.esm.json && tsc-alias -p tsconfig.esm.json",
    "postbuild": "echo '{\"type\": \"module\"}' > ./dist/esm/package.json",
    "type:check": "tsc --noEmit",
    "test": "vitest",
    "prettier:write": "prettier --write .",
    "prettier:check": "prettier --check ."
  },
  "keywords": [
    "arize",
    "datasets",
    "experiments"
  ],
  "devDependencies": {
    "@vitest/coverage-v8": "4.0.10",
    "nock": "^14.0.10",
    "openapi-typescript": "^7.10.1",
    "vitest": "^4.0.10"
  },
  "dependencies": {
    "openapi-fetch": "^0.15.0"
  },
  "packageManager": "pnpm@10.20.0",
  "engines": {
    "node": ">=18"
  }
}
